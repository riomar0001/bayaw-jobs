    "eslint-plugin-prettier": "^5.5.5",
    "nodemon": "^3.1.11",
    "pettier": "^1.1.0",
    "prisma": "^6.19.2",
    "supertest": "^7.2.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
