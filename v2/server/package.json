{
  "name": "project-cert",
  "version": "1.0.0",
  "description": "Certification API built with Express and TypeScript",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec tsx src/server.ts",
    "build": "tsup",
    "start": "node dist/server.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check .",
    "format:check:ci": "./node_modules/.bin/prettier --check .",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "express",
    "Certification",
    "API"
  ],
  "author": "riomar0001",
  "license": "ISC",
  "type": "module",
  "dependencies": {
